        "version": "KqlItem/1.0",
        "query": "OCILogs\r\n| where data_eventName_s =~ 'InteractiveLogin'\r\n| project TimeGenerated, SourceAddress = SrcIpAddr, User = data_stateChange_current_userName_s",
