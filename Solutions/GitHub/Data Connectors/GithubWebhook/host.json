{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.3.0, 4.0.0)"
  },
  "functionTimeout": "00:10:00",
  "extensions": {
        "http": {
            "routePrefix": "github"
        }
    },
    "retry": {
      "strategy": "fixedDelay",
      "maxRetryCount": 3,
      "delayInterval": "00:00:10"
  }

}
